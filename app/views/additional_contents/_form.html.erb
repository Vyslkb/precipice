

<div class="row">
	<div class="col-md-10">
		<%= form_for(@additional_content, html:{role:"form"}) do |f| %>
		  <% if @additional_content.errors.any? %>
		    <div id="error_explanation">
		      <h2><%= pluralize(@additional_content.errors.count, "error") %> prohibited this additional_content from being saved:</h2>
		
		      <ul>
		      <% @additional_content.errors.full_messages.each do |msg| %>
		        <li><%= msg %></li>
		      <% end %>
		      </ul>
		    </div>
		  <% end %>
		 <div class="form-group">
		    <%= f.label :prints_and_pricing %><br>
		    <%= f.text_area :prints_and_pricing, rows: 10, :class => 'form-control' %>
		  </div>
		  <div class="form-group">
		    <%= f.label :philosophy %><br>
		    <%= f.text_area :philosophy,rows: 10, :class => 'form-control' %>
		  </div>
		  <div class="form-group">
		    <%= f.label :travel_quote, "Travel Quotes" %><br>
		    <%= f.text_area :travel_quote,rows: 10, :class => 'form-control' %>
		  </div>
		  <div class="form-group">
		    <%= f.label :contact %><br>
		    <%= f.text_area :contact,rows: 10, :class => 'form-control' %>
		  </div>
		 
		  <div class="actions">
		   <button type="submit" class="btn btn-default">Save</button>
		  </div>
		<% end %>
	</div>
	
</div>


