<h1>Listing print_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Display order</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @print_types.each do |print_type| %>
      <tr>
        <td><%= print_type.name %></td>
        <td><%= print_type.display_order %></td>
        <td><%= link_to 'Show', print_type %></td>
        <td><%= link_to 'Edit', edit_print_type_path(print_type) %></td>
        <td><%= link_to 'Destroy', print_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Print type', new_print_type_path %>
