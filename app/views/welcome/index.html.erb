<div class="row">
	<div class="col-md-12">
		<div class="center-block">
		
		<div id="carousel-example-generic" class="carousel carousel-fade slide" id="welcome-carousel">
		<!-- Wrapper for slides -->
		  <div class="carousel-inner">
		  
			<% Photo.slideshow.each_with_index do |photo, index|%>
		      	<div class="item <%= "active" if index == 0%>"><!-- class of active since it's the first item -->
		      		  <%= image_tag photo.photo_file.url(:large), class: "img-rounded maximize_size", height: "400", width:"600" %>
				      <div class="carousel-caption">
				        <p></p>
				      </div>
		    	</div>
			<%end%>  	
		  </div>
		</div>
		</div>
	</div>
</div>