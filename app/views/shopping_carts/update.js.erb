
$('#show_cart_container').html("<%= escape_javascript(render :partial => 'show_cart') %>");

shopping_cart.update_quantity_ui();

<% if @update_quantity == true %>
	$('[data-content="saved-msg"]').filter('[data-id="<%= params[:shopping_cart_item_id]%>"]').toggleClass("hidden");
<%end%>

<% if @error == true %>
	$('#discount_code_message').html("<%= @discount_error_msg%>");
	$('#discount_code_message').addClass('text-danger');
<% end %>
 



